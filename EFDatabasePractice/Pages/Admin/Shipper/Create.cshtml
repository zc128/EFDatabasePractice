@page
@model EFDatabasePractice.Pages.Admin.Shipper.ShipperCreateModel
@{
}
<div class="create-form">
<form method="post">
    <div>
        <h4>Add New Shipper:</h4>
        <label asp-for="Shipper.CompanyName"></label>
        <input asp-for="Shipper.CompanyName" />
        <span asp-Validation-for="Shipper.CompanyName"></span>
    </div>
    <div>
        <label asp-for="Shipper.Phone"></label>
        <input asp-for="Shipper.Phone" />
        <span asp-Validation-for="Shipper.Phone"></span>
    </div>
    <div asp-Validation-summary="All"></div>
    <input type="submit" value="Create Shipper">
</form>
</div>

@section Scripts
{
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>

    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}